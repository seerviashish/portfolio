{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@react-three/a11y": "^3.0.0",
    "@react-three/drei": "^9.88.2",
    "@react-three/fiber": "^8.14.5",
    "@react-three/gltfjsx": "^4.3.4",
    "@react-three/rapier": "^1.1.2",
    "@tanstack/react-query": "^4.29.15",
    "@tanstack/react-query-devtools": "^4.29.15",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.6",
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.2.13",
    "@types/three": "^0.157.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "tailwindcss": "^3.3.3",
    "three": "^0.157.0",
    "typescript": "5.2.2",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "commit": "git-cz",
    "start": "react-scripts start",
    "i18n:extract": "lingui extract",
    "i18n:compile": "lingui compile",
    "build:app": "react-scripts build",
    "build:i18n": "yarn i18n:extract && yarn i18n:compile",
    "build": "yarn build:i18n && yarn build:app",
    "test": "react-scripts test",
    "test:nowatch": "react-scripts test --watchAll=false --passWithNoTests",
    "eject": "react-scripts eject",
    "postinstall": "husky install",
    "pre-commit": "lint-staged && yarn test:nowatch",
    "eslint:check": "eslint src/**/*.{js,jsx,ts,tsx}",
    "eslint:fix": "eslint --fix src/**/*.{js,jsx,ts,tsx}",
    "prettier:check": "prettier --check . --config ./.prettierrc.js",
    "prettier:write": "prettier --write . --config ./.prettierrc.js --ignore-unknown",
    "lint:check": "yarn eslint:check && yarn prettier:check",
    "lint:fix": "yarn eslint:fix && yarn prettier:write",
    "lint-staged:check": "lint-staged"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@lingui/cli": "^4.5.0",
    "@lingui/core": "^4.2.1",
    "@lingui/macro": "^4.5.0",
    "@lingui/react": "^4.5.0",
    "@tanstack/eslint-plugin-query": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.60.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "git-cz": "^4.9.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.1",
    "prettier": "^2.8.8"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint"
    ],
    "*": [
      "prettier --check --ignore-unknown"
    ]
  }
}

    "@types/three": "^0.156.0",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@webpack-cli/generators": "^3.0.7",
    "autoprefixer": "^10.4.16",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "file-loader": "^6.2.0",
    "git-cz": "^4.9.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "javascript-obfuscator": "^4.1.0",
    "lint-staged": "^14.0.1",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "prettier": "^3.0.3",
    "serve": "^14.2.1",
    "style-loader": "^3.3.3",
    "stylus": "^0.60.0",
    "stylus-loader": "^7.1.3",
    "terser-webpack-plugin": "^5.3.9",
    "three": "^0.157.0",
    "ts-loader": "^9.4.4",
    "tweakpane": "^4.0.1",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-obfuscator": "^3.5.1",
    "workbox-webpack-plugin": "^7.0.0"
  },
  "description": "3D Portfolio",
  "scripts": {
    "commit": "git-cz",
    "build": "yarn build:prod",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "serve": "webpack serve",
    "build:serve": "serve -S dist",
    "postinstall": "husky install",
    "pre-commit": "lint-staged",
    "eslint:check": "eslint src/**/*.{js,ts}",
    "eslint:fix": "eslint --fix src/**/*.{js,ts}",
    "prettier:check": "prettier --check . --config ./.prettierrc.js",
    "prettier:write": "prettier --write . --config ./.prettierrc.js --ignore-unknown",
    "lint:check": "yarn eslint:check && yarn prettier:check",
    "lint:fix": "yarn eslint:fix && yarn prettier:write",
    "lint-staged:check": "lint-staged"
  },
  "dependencies": {
    "@lingui/core": "^4.5.0",
    "tailwindcss": "^3.3.3"
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "eslint"
    ],
    "*": [
      "prettier --check --ignore-unknown"
    ]
  }
}
